include cluster.pug
include groups.pug
include spreadsheet.pug
include staff.pug
include table.pug

mixin dispatch(value)
  if value.type == 'Error'
    pre= JSON.stringify(value.data, null, 2)
  if value.type == 'Exception'
    pre= value.data
  if value.type == 'Table'
    +table(value.data)
  if value.type == 'String'
    div= value.data
  if value.type == 'Header'
    h3= value.data
  if value.type == 'ClusteringResult'
    +clusters(value.data)
  if value.type == 'GroupAssignmentResult'
    +groups(value.data)
  if value.type == 'ReadSpreadsheetResult'
    +spreadsheet(value.data)
  if value.type == 'StaffAssignmentResult'
    +staff(value.data)
  if value.type == 'Multi'
    each data in value.data
      +dispatch(data)
  if value.type.startsWith('Array<')
    each data in value.data
      +dispatch({ type: value.type.substr(6, value.type.length - 7), value: data })
  if value.type == 'Person'
    div= value.data.name
