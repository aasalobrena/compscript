mixin table(data)
  if data.rows.length == 0
    i No rows were returned
  table
    tr
      each header in data.headers
        th= header
    each row in data.rows
      tr
        each cell in row
          td
            if cell.link
              a(href=cell.link)= cell.value.toString()
            else
              =cell.value.toString()
